      "n = int(raw_input())\n",
      "for x in xrange(n):\n",
      "    cont = 0\n",
      "    a, b = map(int, raw_input().split(' '))\n",
      "    while a != b:\n",
      "        if a > b:\n",
      "            a /= 2\n",
      "            cont += 1\n",
      "        elif a < b:\n",
      "            a += 1\n",
      "            cont += 1\n",
      "    print cont"
